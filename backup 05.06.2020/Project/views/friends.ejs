        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script> 

        <style type="text/css">
            body{
                color: #fff;
                background: #63738a;
                font-family: 'Lato', sans-serif;
                padding-top: 50px;
                position: relative;
                
            }
      
            pre {
                tab-size: 8;
            }
            
            @media screen and (max-width: 768px) {
                .side-collapse-container{
                    width:100%;
                    position:relative;
                    left:0;
                    transition:left .4s;
                }
                .side-collapse-container.out{
                    left:200px;
                    
                }
                .side-collapse {
                    top:50px;
                    bottom:0;
                    left:0;
                    width:200px;
                    position:fixed;
                    overflow:hidden;
                    transition:width .4s;
                }
                .side-collapse.in {
                    width:0;
                }
            }
        </style>
    </head>
